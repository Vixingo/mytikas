<div class="leads_header d-flex justify-content-between align-items-center">
    <div class="lead_left">
        <h4 class="fw-light">Contacts</h4>
        <p style="color: #707070">
            7 Total Results &nbsp; &bull; &nbsp; <a href="#">Set limits</a>
        </p>
    </div>
    <div class="lead_right">
        <button
            class="btn btn-sm"
            data-bs-toggle="tooltip"
            data-bs-placement="top"
            title="Click to copy link"
        >
            <i class="fas fa-file-import"></i> &nbsp; Share Links
        </button>
        <div class="btn-group">
            <button
                class="btn btn-sm"
                type="button"
                style="border: 1px solid rgba(0, 0, 0, 0.2)"
                data-bs-toggle="tooltip"
                data-bs-placement="top"
                title="Bulk email these Contacts..."
            >
                <i class="fas fa-envelope"></i> &nbsp; Email
            </button>
            <button
                type="button"
                class="btn dropdown-toggle dropdown-toggle-split btn-sm"
                data-bs-toggle="dropdown"
                aria-expanded="false"
                style="border: 1px solid rgba(0, 0, 0, 0.2)"
            >
                <i class="fas fa-caret-down"></i>
            </button>
            <ul class="dropdown-menu">
                <li class="mx-3">
                    <i class="fas fa-envelope"></i>&nbsp; Bulk Email
                </li>
            </ul>
        </div>
        <button
            class="btn"
            data-bs-toggle="tooltip"
            data-bs-placement="top"
            title="More"
        >
            <i class="fas fa-ellipsis-h"></i>
        </button>
    </div>
</div>
<hr />
